    <%= form_for(:session, url: sessions_path) do |f| %>
      <%= f.label :phone %>
      <%= f.text_field :phone,placeholder: "input your phone" %>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.label :remember_me %>
      <%= f.check_box :remember_me %>
      <%= f.submit "Log in" %>
    <% end %>
   